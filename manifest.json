{
  "manifest_version": 2,
  "name": "Website Blocker",
  "version": "1.0",
  "description": "Extension that aids in website self-control",
  "browser_action": {
    "default_icon": "icon.png",
	  "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "webNavigation",
	  "http://*.googleapis.com/*",
	  "tabs",
	  "bookmarks"
  ]
}
